<rxui:ReactiveContentPage 
             xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:rxui="clr-namespace:ReactiveUI.Maui;assembly=ReactiveUI.Maui"
             xmlns:vms="clr-namespace:MauiApp5.ViewModels"
             x:Class="MauiApp5.AnotherPage"
             x:TypeArguments="vms:AnotherViewModel">

    <StackLayout>
        <Label Text="Hello from another view model!" />

		<Button x:Name="BackBtn"
                Text="Back"
                FontAttributes="Bold"
                HorizontalOptions="Center"
                VerticalOptions="Center"/>
		
		<Button x:Name="ToMainBtn"
                Text="To Main Page"
                FontAttributes="Bold"
                HorizontalOptions="Center"
                VerticalOptions="Center"/>

		<Button Text="PopToRoot()"
                FontAttributes="Bold"
                HorizontalOptions="Center"
                VerticalOptions="Center"
				Clicked="Button_Clicked"
				/>

	</StackLayout>
</rxui:ReactiveContentPage>
