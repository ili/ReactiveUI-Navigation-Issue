<rxui:ReactiveContentPage 
             xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:rxui="clr-namespace:ReactiveUI.Maui;assembly=ReactiveUI.Maui"
             xmlns:vms="clr-namespace:MauiApp5.ViewModels"
             x:Class="MauiApp5.MainPage"
             x:TypeArguments="vms:MainPageViewModel">

	<StackLayout>
        <Button x:Name="NavigateBtn"
                Text="Navigate 1"
                FontAttributes="Bold"
                HorizontalOptions="Center"
                VerticalOptions="Center"/>

		<Button x:Name="NavigateBtn2"
                Text="Navigate 2"
                FontAttributes="Bold"
                HorizontalOptions="Center"
                VerticalOptions="Center"/>

		<Button x:Name="NavigateBtn3"
                Text="Navigate 3"
                FontAttributes="Bold"
                HorizontalOptions="Center"
                VerticalOptions="Center"/>
	</StackLayout>
</rxui:ReactiveContentPage>
